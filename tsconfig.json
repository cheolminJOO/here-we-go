{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "types": ["@emotion/react/types/css-prop"]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/test/index.tsx",
    "src/componets/commons/layout/banner/LayoutBanner.container.js",
    "src/componets/commons/layout/navigation/LayoutNavigation.container.js",
    "src/componets/units/board/openapi/list/OpenapiList.presenter.tsx",
    "src/componets/units/board/comment/list/BoardCommentList.presenter.tsx",
    "src/componets/units/item/detail/ItemDetail.queries.ts",
    "pages/item/[boardId]/edit/index.js",
    "src/componets/units/board/comment/list2/BoardCommentList.container.js",
    "src/componets/units/item/comment/write/write/BoardCommentWrite.presenter.js",
    "src/componets/units/item/comment/write/write/BoardCommentWrite.container.js",
    "src/componets/units/board/list/BoardList.presenter.tsx",
    "src/componets/units/board/list/BoardList.container.tsx",
    "pages/mypage/index.tsx",
    "src/componets/units/board/openapi/list/OpenapiList.container.tsx",
    "src/componets/units/item/comment/list/ItemCommentList.presenterItem.tsx",
    "pages/boards/index.js",
    "src/componets/units/item/write/ItemWrite.presenter.tsx",
    "pages/boards/[boardId]/edit/index.js",
    "pages/boards/new/index.js",
    "pages/boards/[boardId]/index.js",
    "pages/item/index.tsx"
  ],
  "exclude": ["node_modules"]
}
